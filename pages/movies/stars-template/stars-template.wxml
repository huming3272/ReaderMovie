<!--pages/stars/stars.wxml-->
<template name="starsTemplate">
	<view class="stars-container">
		<view class="stars">
			<block wx:for="{{stars}}" wx:for-item="item">
				<image wx:if="{{item}}" mode="widthFix" src="/pages/Image/icon/star.png"></image>
				<image wx:else mode="widthFix" src="/pages/Image/icon/none-star.png"></image>
			</block>
			
		</view>
		<text>{{average}}</text>
	</view>
</template>